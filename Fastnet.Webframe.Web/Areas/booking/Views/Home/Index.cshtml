@using Fastnet.Webframe.BookingData
@{
    ViewBag.Title = "Booking";
    ViewBag.AppName = BookingGlobals.GetLodgementName() +  " Booking System";
}
<div class="row">
    <div id="bookingCalendar"></div>
</div>
<div class="row">
    <div class="booking-interaction"></div>
</div>

@section scripts {
    @Scripts.Render("~/bundles/booking")
    <script>
        var bookingApp = null;
        (function ($) {
            @*var _u = fastnet.utilities.getInstance();
            _u.setBaseUrl('@Url.Content("~/")');*@
            bookingApp = new fastnet.bookingApp();
            bookingApp.start();
        })(jQuery);
    </script>
}